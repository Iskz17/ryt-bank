{
  "expo": {
    "name": "ryt-bank",
    "slug": "ryt-bank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0000e6"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.jpg",
        "backgroundColor": "#0000e6"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
